{
    "name": "<%= name %>",
    "version": "0.1.0",
    "description": "<%= description %>",
    "dependencies": {
        "jquery": "~2.1.0",
        "lodash": "~3.5.0",
        "backbone": "~1.1.2",
        "backbone.radio": "~0.9.0",
        "backbone.marionette": "~2.4.1", <% if (engine === 'dust') { %>
        "dustjs-linkedin": "^2.4.0",
        "dustjs-helpers": "^1.2.0",
        "backbone.marionette.dust": "~0.1.6", <% } else { %>
        "handlebars": "~3.0.0", <% } %>
        "moment": "~2.8.1"
    },
    "browser": {
        "jquery":              "./node_modules/jquery/dist/jquery.js",
        "lodash":              "./node_modules/lodash/index.js",
        "backbone":            "./node_modules/backbone/backbone.js",
        "backbone.marionette": "./node_modules/backbone.marionette/lib/backbone.marionette.js"
    },
    "browserify-shim": "./config/shim.js",
    "browserify": {
        "transform": [
            "browserify-shim",
            "browserify-data",<% if (engine === 'dust') { %>
            "browserify-dustjs", <%} else {%>
            "hbsfy", <% } %>
            "aliasify"
        ]
    },
    "aliasify": {
        "aliases": {
            "underscore": "lodash",
            "app": "./src/scripts/app"
        }
    },
    "devDependencies": {
        "aliasify": "^1.5.1",
        "mochify": "^2.6.1", <% if (engine === 'handlebars') { %>
        "hbsfy": "^2.2.1", <% } %>
        "jsdom": "^4.0.4",
        "browser-sync": "~2.0.1",
        "browserify": "~8.0.3",
        "browserify-data": "^0.2.0",
        "browserify-global-shim": "^1.0.0",
        "browserify-shim": "~3.8.2", <% if (engine === 'dust') { %>
        "browserify-dustjs": "^0.0.5", <% } %>
        "del": "^1.1.1",
        "glob": "^4.3.2",
        "gulp": "^3.8.7",
        "gulp-autoprefixer": "^2.0.0",
        "gulp-changed": "^1.1.1",
        "gulp-if": "^1.2.5",
        "gulp-imagemin": "^0.6.2",
        "gulp-jasmine-phantom": "^1.1.1",
        "gulp-jshint": "^1.9.0",
        "gulp-minify-css": "^0.3.13",
        "gulp-notify": "^1.4.2",
        "gulp-plumber": "^0.6.6",
        "gulp-sass": "^1.3.3",
        "gulp-scss-lint": "^0.1.7",
        "gulp-sourcemaps": "^1.2.8",
        "gulp-unretina": "^0.1.2",
        "gulp-util": "^3.0.0",
        "gulp-messageformat": "^0.0.5",
        "jshint-stylish": "^1.0.0",
        "pretty-hrtime": "~0.2.1",
        "require-dir": "^0.1.0",
        "uglifyify": "^2.6.0",
        "vinyl-source-stream": "~0.1.1",
        "watchify": "~2.2.1",
        "mocha": "*",
        "chai": "*",
        "sinon": "^1.14.1"
    }
}
